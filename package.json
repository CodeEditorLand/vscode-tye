			{
				"contents": "%vscode-tye.views.services.contents.notRunning%",
				"view": "vscode-tye.views.services",
				"when": "vscode-tye.views.services.state == 'notRunning'"
			}
		],
		"yamlValidation": [
			{
				"fileMatch": [
					"tye.yml",
					"tye.yaml"
				],
				"url": "./resources/schemas/tye.json"
			}
		]
